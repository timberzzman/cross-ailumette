{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "globals": {
        "window": true,
        "document": true,
        "navigator": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
        "import/no-extraneous-dependencies": "off"
    }
}
